{
  "name": "strider-android",
  "version": "1.0.1",
  "description": "A Strider plugin for Android testing",
  "main": "webapp.js",
  "scripts": {
    "test": "mocha -R spec --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crokita/strider-android"
  },
  "keywords": [
    "strider",
    "plugin",
    "template"
  ],
  "author": "Chris Rokita",
  "license": "ISC",
  "dependencies": {
    "async": "^0.9.0"
  },
  "strider": {
    "type": "job",
    "id": "android",
    "title": "Android",
    "worker": "worker.js",
    "webapp": "webapp.js",
    "icon": "icon.png",
    "config": {
      "controller": "AndroidController"
    }
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "mocha": "^1.20.1",
    "sinon-chai": "^2.5.0"
  }
}
